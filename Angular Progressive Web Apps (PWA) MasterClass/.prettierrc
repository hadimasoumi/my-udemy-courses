{
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "printWidth": 150,
        "wordWrapColumn": 150,
        "semi": true,
        "singleQuote": true,
        "arrowParens": "avoid"
      }
    },
    {
      "files": "*.html",
      "options": {
        "wordWrapColumn": 80,
        "singleAttributePerLine": true
      }
    }
  ]
}
